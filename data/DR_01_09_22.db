-- Table: predictions
DROP TABLE IF EXISTS predictions;

CREATE TABLE predictions (
    event_id        INTEGER PRIMARY KEY,
    evn_num         TEXT,
    evn_nam         TEXT,
    start_dt        TEXT,
    end_dt          TEXT,
    perf            REAL,
    nomination      REAL,
    factor          REAL,
    unadj_credit    REAL,
    del_load_dist   REAL,
    adj_nom         REAL,
    adj_per_fact    REAL,
    tot_crdt_paid   REAL,
    est_vec_dem_sav REAL,
    event           TEXT,
    credit_rate     REAL,
    whisl_dem_cost  REAL,
    crtd_dt         TEXT,
    upd_dt          TEXT,
    customer_id     INTEGER REFERENCES customer (customer_id) 
);


-- Table: rawdata
DROP TABLE IF EXISTS rawdata;

CREATE TABLE rawdata (
    id   INTEGER,
    type TEXT,
    data BLOB
);


